class Student
{ String name;
int age;
String dept;

public Student(String name,int age,String dept)
{
this.name=name;
this.age=age;
this.dept=dept; }

public void Displaydetails()
{ 
System.out.println("Student name = " + name);
System.out.println("Student age = " + age);
System.out.println("Student dept = " + dept);
} 
}
public class task5
{ public static void main(String[] args)
{
Student std = new Student("anushka", 18,"EXTC");
std.Displaydetails();
}
}
